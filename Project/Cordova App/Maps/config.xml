<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.Maps" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Stay Safe</name>
    <description>
        Your friend on roads.
    </description>
    <author email="udityalaad123@gmail.com" href="http://cordova.io">
        Uditya Laad and Team
    </author>
    <content src="index.html" />
    <icon src="res/App_Icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <allow-intent href="schemename://hostname/path" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="kr.co.joycorp.cordova.exitapp" spec="~1.0.0" />
    <plugin name="cordova-plugin-media-capture" spec="^3.0.2" />
    <plugin name="cordova-plugin-media" spec="^5.0.2" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="windows" spec="^6.0.1" />
</widget>
