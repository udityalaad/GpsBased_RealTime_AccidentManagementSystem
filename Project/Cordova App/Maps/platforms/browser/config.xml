<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.Maps" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Stay Safe</name>
    <description>
        Your friend on roads.
    </description>
    <author email="udityalaad123@gmail.com" href="http://cordova.io">
        Uditya Laad and Team
    </author>
    <content src="index.html" />
    <icon src="res/App_Icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
